@{
    ViewBag.Title = "About";
}
<h2>@ViewBag.Title.</h2>
<h3>@ViewBag.Message</h3>

<div class="jumbotron">
    <h1>Hello, you are logged in as</h1>
    <p class="lead">@User.Identity.Name!</p>
</div>

<div class="row">
    <div class="col-md-4">
        <h2>Let's try download something</h2>
        <p>
            @Html.ActionLink("Download", "Download", "Home", new { @class = "btn btn-default" })
        </p>
    </div>
</div>
